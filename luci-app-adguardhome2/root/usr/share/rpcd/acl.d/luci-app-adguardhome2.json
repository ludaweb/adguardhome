{
	"luci-app-adguardhome2": {
		"description": "Grant UCI access for luci-app-adguardhome2",
		"read": {
            "file": {
                "/usr/share/AdGuardHome2/addhost.sh": [ "exec" ],
                "/usr/share/AdGuardHome2/AdGuardHome2_template.yaml": [ "read" ],
                "/usr/share/AdGuardHome2/firewall.start": [ "read" ],
                "/usr/share/AdGuardHome2/getsyslog.sh": [ "exec" ],
                "/usr/share/AdGuardHome2/gfw2adg.sh": [ "exec" ],
                "/usr/share/AdGuardHome2/gfwipset2adg.sh": [ "exec" ],
                "/usr/share/AdGuardHome2/links.txt": [ "read" ],
                "/usr/share/AdGuardHome2/tailto.sh": [ "exec" ],
                "/usr/share/AdGuardHome2/update_core.sh": [ "exec" ],
                "/usr/share/AdGuardHome2/waitnet.sh": [ "exec" ],
                "/usr/share/AdGuardHome2/watchconfig.sh": [ "exec" ],
                "/etc/init.d/AdGuardHome2": [ "exec" ],
                "/etc/init.d/dnsmasq": [ "exec" ],
                "/usr/bin/AdGuardHome2/AdGuardHome2": [ "exec" ]
            },
			"uci": [ "AdGuardHome2", "adguardhome" ]
		},
		"write": {
            "file": {
                "/usr/share/AdGuardHome2/AdGuardHome2_template.yaml": [ "write" ],
                "/usr/share/AdGuardHome2/firewall.start": [ "write" ],
                "/usr/share/AdGuardHome2/links.txt": [ "write" ]
            },
			"uci": [ "AdGuardHome2", "adguardhome" ]
		}
	}
}
